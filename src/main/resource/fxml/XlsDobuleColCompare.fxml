<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import sample.fxml.componet.fxml.FileSelector?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sample.fxml.controllers.XlsDoubleColCompareController"
            prefHeight="400.0" prefWidth="600.0">
    <VBox spacing="10" AnchorPane.topAnchor="10" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0"
          AnchorPane.rightAnchor="0" alignment="TOP_RIGHT">
        <FileSelector labelWidth="140" label="Xls目录" folder="false" fx:id="xlsFileSelector" configType="XLS_DOUBLE_COL_COMPARE_PATH"
                      extendStr=".xls"/>
        <Button text="解析" onMouseClicked="#onStartCompareBtnClick"/>

    </VBox>
</AnchorPane>
